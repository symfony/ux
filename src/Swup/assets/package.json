{
    "name": "@symfony/ux-swup",
    "description": "Swup integration for Symfony",
    "license": "MIT",
    "version": "1.1.0",
    "module": "dist/controller.js",
    "type": "module",
    "types": "dist/controller.d.ts",
    "symfony": {
        "controllers": {
            "swup": {
                "main": "dist/controller.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": true
            }
        },
        "importmap": {
            "@swup/fade-theme": "^1.0",
            "@swup/slide-theme": "^1.0",
            "@swup/forms-plugin": "^2.0",
            "@swup/debug-plugin": "^3.0",
            "swup": "^3.0",
            "@hotwired/stimulus": "^3.0.0"
        }
    },
    "peerDependencies": {
        "@swup/fade-theme": "^1.0",
        "@swup/slide-theme": "^1.0",
        "@swup/forms-plugin": "^2.0",
        "@swup/debug-plugin": "^3.0",
        "swup": "^3.0",
        "@hotwired/stimulus": "^3.0.0"
    },
    "devDependencies": {
        "@swup/fade-theme": "^1.0",
        "@swup/slide-theme": "^1.0",
        "@swup/forms-plugin": "^2.0",
        "@swup/debug-plugin": "^3.0",
        "swup": "^3.0",
        "@hotwired/stimulus": "^3.0.0"
    }
}
