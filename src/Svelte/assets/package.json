        }
    },
    "peerDependencies": {
        "@hotwired/stimulus": "^3.0.0",
        "svelte": "^3.0 || ^4.0"
    },
    "devDependencies": {
        "@hotwired/stimulus": "^3.0.0",
        "@sveltejs/vite-plugin-svelte": "^2.4.6",
        "@types/webpack-env": "^1.16",
        "svelte": "^3.0 || ^4.0"
    }
}
