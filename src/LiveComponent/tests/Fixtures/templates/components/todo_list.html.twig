<div{{ attributes }}>
    Todo List: {{ name }}

    <ul>
    {% for item in items %}
        {% set componentProps = { text: item.text } %}
        {% if includeDataLiveId %}
            {% set componentProps = componentProps|merge({'data-live-id': ('todo-item-' ~ loop.index) }) %}
        {% endif %}
        {{ component('todo_item', componentProps) }}
    {% endfor %}
    </ul>
</div>
